<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="console_number_view_list" model="ir.ui.view">
            <field name="name">console_number_view_list</field>
            <field name="model">console.number</field>
            <field name="arch" type="xml">
                <list editable="bottom">
                    <field name="sequence" widget="handle"/>
                    <field name="device_num"/>
                    <field name="type_id"/>
                </list>
            </field>
        </record>


        <record id="console_number_action" model="ir.actions.act_window">
            <field name="name">Console Number</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">console.number</field>
            <field name="view_mode">list</field>
        </record>

    </data>
</odoo>