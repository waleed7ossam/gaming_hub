<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="cafe_number_view_list" model="ir.ui.view">
            <field name="name">cafe_number_view_list</field>
            <field name="model">cafe.table</field>
            <field name="arch" type="xml">
                <list editable="bottom">
                    <field name="sequence" widget="handle"/>
                    <field name="table_num"/>
                </list>
            </field>
        </record>


        <record id="cafe_table_number_action" model="ir.actions.act_window">
            <field name="name">Table Number</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cafe.table</field>
            <field name="view_mode">list</field>
        </record>

    </data>
</odoo>